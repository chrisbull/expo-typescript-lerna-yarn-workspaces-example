{
  "name": "siteappy-expo-yarn-workspace",
  "version": "1.0.0",
  "author": "Chris Bull <chris@chrisbull.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ios": "lerna run ios --scope=mobile --stream --",
    "android": "lerna run android --scope=mobile --stream --",
    "lint": "yarn eslint && yarn tslint",
    "eslint": "eslint --fix --ext js --ext jsx --ext json .",
    "tslint": "tslint --fix -p tsconfig.json -c tslint.json",
    "format": "prettier **/*.{ts,js,tsx,jsx} --write"
  },
  "workspaces": [
    "packages/*",
    "tools/expo-yarn-workspaces"
  ],
  "cacheDirectories": [
    ".cache"
  ],
  "devDependencies": {
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/prettier": "^1.16.1",
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.8.3",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "expo-yarn-workspaces": "^1.1.0",
    "lerna": "^3.13.1",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.1",
    "tslint": "^5.14.0",
    "tslint-auto-fix": "^1.0.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-prettiest": "^0.0.1",
    "tslint-react": "^3.6.0",
    "tslint-react-native": "^0.0.6",
    "typescript": "^3.3.4000",
    "typescript-styled-plugin": "^0.14.0",
    "typescript-tslint-plugin": "^0.3.1",
    "typesync": "^0.5.1"
  }
